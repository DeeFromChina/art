<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="publication" extends="default" namespace="/publication">
		<action name="artPublication" class="com.golead.art.publication.ArtPublicationAction">
			<result name="LIST">/jsp/publication/art_publication_list.jsp</result>
			<result name="ADD">/jsp/publication/art_publication_add.jsp</result>
			<result name="EDIT">/jsp/publication/art_publication_edit.jsp</result>
			<result name="VIEW">/jsp/publication/art_publication_view.jsp</result>
		</action>
		<action name="pubLiterature" class="com.golead.art.publication.ArtPublicationLiteratureAction">
			<result name="LIST">/jsp/publication/pubLiterature_list.jsp</result>
			<result name="ADD">/jsp/publication/pubLiterature_add.jsp</result>
			<result name="EDIT">/jsp/publication/pubLiterature_edit.jsp</result>
			<result name="VIEW">/jsp/publication/pubLiterature_view.jsp</result>
		</action>
		<action name="pubWorks" class="com.golead.art.publication.ArtPublicationWorksAction">
			<result name="LIST">/jsp/publication/pubWorks_list.jsp</result>
		</action>
	</package>
