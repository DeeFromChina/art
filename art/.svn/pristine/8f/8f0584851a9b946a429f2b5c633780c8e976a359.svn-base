.div_cell {
	width: 100%;
	height: 100%;
	border:0px solid #999999;
   font-size:12px;
}

.div_cell_frame {
	width: 100%;
	height: 100%;
   padding-top:0px;
   padding-left:0px;
   font-size:12px;
}

.div_cell div.in_cell{
   position:relative;
	text-align:center;
	border-top:1px solid #999999;
	border-left:1px solid #999999;
	border-right:1px solid #999999;
	border-bottom:1px solid #999999;
	background-color: #e8e8e8;
}

.div_cell div.in_cellImg{
   position:relative;
	text-align:center;
	border-top:1px solid #999999;
	border-left:1px solid #999999;
	border-right:1px solid #999999;
	border-bottom:1px solid #999999;
	background-color: #e8e8e8;
	background-image: url("chart_bar_link20.png");
	background-repeat: no-repeat;
	background-position: right bottom; 
}

.div_cell div.in_cell div.realdiv {
	text-align:center;
	border:0px;
	background-color: red;
}

.div_cell div.in_cell_noborder{
	text-align:center;
	border-top:0px solid #999999;
	border-left:0px solid #999999;
	border-right:0px solid #999999;
	border-bottom:0px solid #999999;
}

