html,body {
	width: 100%;
	height: 100%;
	border: none;
	margin: 0px;
	background-color: white;
}

.indCssSelect {
	background: #d0f8d0
}

.indCssSelect b.rtop {
	display: block;
	background: #FFFFFF
}

.indCssSelect b.rtop b {
	display: block;
	height: 1px;
	overflow: hidden;
	background: #d0f8d0
}

.indCssSelect b.rbottom {
	display: block;
	background: #FFFFFF
}

.indCssSelect b.rbottom b {
	display: block;
	height: 1px;
	overflow: hidden;
	background: #d0f8d0
}

.indCssSelect b.r1 {
	margin: 0 5px
}

.indCssSelect b.r2 {
	margin: 0 3px
}

.indCssSelect b.r3 {
	margin: 0 2px
}

.indCssSelect b.rtop b.r4 {
	margin: 0 1px;
	height: 2px
}

.indCssSelect b.rbottom b.r4 {
	margin: 0 1px;
	height: 2px
}

.indCssSelect div.innerArea {
	margin-left: 4px;
	margin-right: 4px;
	text-align: center;
	padding-top: 4px;
}

.indCssSelect div.innerArea td.dataArea {
	font-size: 24px;
	font-weight: bold;
	text-align: center;
	vertical-align: middle;
	color: red;
	vertical-align: middle;
}

.indCssSelect div.innerArea td.dataArea span.dataMain {
	font-size: 24px;
	font-weight: bold;
	color: red;
	line-height: 24px;
}

.indCssSelect div.innerArea td.dataArea span {
	font-size: 14px;
	font-weight: normal;
	line-height: 24px;
	color: black;
}

.indCssSelect div.innerArea td.dataTitle {
	font-size: 20px;
	font-family:楷体,宋体;
	font-weight: bold;
	color: red;
	text-align: left;
	vertical-align: middle;
}

.indCssSelect div.innerArea td.dataTitleSub {
	line-height: 16px;
	font-size: 12px;
	text-align: left;
	vertical-align: top;
}

